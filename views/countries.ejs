<%- include ('./header.ejs') %>
      <section style="padding-top: 10rem;background-color: white;" >
        <div class="bg-holder" style="background-image:url();">
        </div>
        <!--/.bg-holder-->

        <div class="container">
          <div class="row align-items-center">
            <div class="col-md-5 col-lg-6 order-0 order-md-1 text-end">
              <img class="pt-7 pt-md-0 hero-img" src="/images/<%=result[3][0].visalogo%>" alt="hero-header" style='height: 400px;border-radius: 16px;'/>
            </div>
            <div class="col-md-7 col-lg-6 text-md-start text-center py-6" style="background:url('/img/Visa/Visa/All Country Page/Heading.png');background-size: contain;background-repeat: no-repeat;">
              <h1 class="hero-titl" style="font-family: Poppins;
              font-size: 48px;
              font-weight: 700;
              line-height: 56px;
              letter-spacing: 0em;
              text-align: left;
              "><%=name.replace('-' , ' ')%></h1>
              <p class="mb-4 fw-medium"><%=result[3][0].visadescription%></p>
              <div class="text-center text-md-start">
                
                <div class="modal fade" id="popupVideo" tabindex="-1" aria-labelledby="popupVideo" aria-hidden="true">
                  <div class="modal-dialog modal-dialog-centered modal-lg">
                    <div class="modal-content">
                      <iframe class="rounded" style="width:100%;max-height:500px;" height="500px" src="https://www.youtube.com/embed/_lhdhL4UDIo" title="YouTube video player" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen="allowfullscreen"></iframe>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>


   
      <!-- ============================================-->
      <!-- <section> begin ============================-->
        <section class="pt-5" id="destination" style="background-color: white;">

          <div class="container" >

            <div class="mb-7 text-center">
              <div class=" translate-middle-x d-none d-xl-block ms-xl-n4 text-center" style="position: absolute;left:62%;top:-40px;align-items: center;align-content: center;z-index: 1;"><img src="/img/Visa/Visa/All Country Page/countryImg.png" alt="destination" style="height: 250px;" /></div>

              <!-- <img src="/img/Visa/Visa/All Country Page/countryImg.png"/> -->
              <h3 class="fs-xl-10 fs-lg-8 fs-7 text-center" style="font-family: Poppins;              
              font-size: 44px;
              font-weight: 700;
              line-height: 56px;
              letter-spacing: 0em;
              text-align: left;
              color: #14183E;
             
            
">
<%=name.replace('-' , ' ')%> Available Countries</h3>
            </div>
            <div class="row">



              <% for(i=0;i<result[3].length;i++) { %>

              <div class="col-md-4 mb-4">
<a href="/visa/<%=name%>/<%=result[3][i].countryname.replace(/[ ,]+/g, "-");%>?visaid=<%=visaid%>&countryid=<%=result[3][i].countryid%>" style='text-decoration:none'>
                <div class="card text-dark bg-white mb-3 card2 shadow" style="height: 476px;border-radius: 16px;">
                  <img src="/images/<%=result[3][i].countrylogo%>" alt="image" style="border-top-left-radius: 10px;border-top-right-radius: 10px;height: 327px;">
                  <div class="card-body">
                    <h3 class="card-title" style="font-family: Poppins;
                    font-size: 20px;
                    font-weight: 500;
                    line-height: 24px;
                    letter-spacing: 0em;
                    text-align: left;
                    "><%=result[3][i].countryname%></h3>
                    <p class="card-text" style="font-family: Poppins;
                    font-size: 14px;
                    font-weight: 400;
                    line-height: 24px;
                    letter-spacing: 0em;
                    text-align: left;
                    overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
                    "><%=result[3][i].countrydescription%></p>
        
                  </div>
                </div>
              </a>
              </div>

              <% } %>

             
              <div class="mt-4 mb-3" style="text-align: center;">
                <!-- <a class="btn btn-primary btn-lg me-md-4 mb-3 mb-md-0 border-0 primary-btn-shadow" style="height: 64px;
                width: 255px;
                border-radius: 30px;
                padding: 16px, 80px, 16px, 80px;
                font-family: Poppins;
font-size: 18px;
font-weight: 600;
line-height: 32px;
letter-spacing: 0px;


                " href="#!" role="button">Load More</a>
               -->
                <!-- <a href="#" class="btn btn-primary stretched-link  button4">See All Countries</a> -->
              </div>
            </div>
          </div><!-- end of .container-->
  
        </section>
        <!-- <section> close ============================-->
        <!-- ============================================-->





        <%- include ('./footer.ejs') %>